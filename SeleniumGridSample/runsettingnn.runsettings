<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <!-- Configurations that affect the Test Framework -->
 <RunConfiguration>
    <DisableParallelization>false</DisableParallelization>
    <MaxCpuCount>10</MaxCpuCount>
  </RunConfiguration>
  <!-- MSTest adapter -->
  <MSTest>
    <Parallelize>
      <Workers>10</Workers>
      <Scope>MethodLevel</Scope>
    </Parallelize>
  </MSTest>
</RunSettings>
